<!DOCTYPE html>
<html>
    <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <title>nohr 好工作就在这里</title>
      <link rel="stylesheet" href="static/css/??reset-min.css"/>
      <link rel="stylesheet/less" type="text/css" href="static/css/home.less"/>
      <script src="static/js/lib/less-1.3.0.min.js"></script>
    </head>
    <body>
        <!--<header id="header"></header>-->
        <div id="search">
            <input type="search" />
        </div>
        <div class="tab">
            <h3 id="zhaopin">招聘信息</h3>
            <h3 id="rencai">人才信息</h3>
        </div>
        <div id="wrapper">
            <div id="zhaopin_items" class="items">
                <% data.forEach(function ( item ) { %>
                    <% item.ctime = item.ctime.slice(0, 10) %>
                    <div>
                        <h3>职位名称: <%= item.jname%></h3>
                        <em>发布时间: <%= item.ctime%></em>
                        <em>招聘人数: <%= item.number%>人</em>
                        <em>工作地点: <%= item.workplace%></em>
                    </div>
                <% }) %>
            </div>
            <div id="rencai_items" class="items">
                
            </div>
        </div>
        <footer>CopyRight @ nohr.com</footer>        
		<script src="static/js/lib/??sea.js,plugin-combo.js"></script>
        <script src="static/js/lib/jquery-1.7.2.min.js"></script>
        <script src="static/js/lib/??underscore.js,backbone.js"></script>
        <script>
            seajs.config({
                /**
                alias: {
                    'jquery': 'jquery-1.7.2.min.js',
                    'uds'   : 'underscore.js',
                    'bb'    : 'backbone.js',
                    'ls'    : 'backbone-localstorage.js'
                    // 'a'     : 'a.js'
                    // 'todos' : '/static/js/todos/todos'
                },
                */
                base: '/static/js',
                debug: true
            }).use('init');
        </script>
        <script id="jobMsgTpl" type="text/template">
            <div id="jobMsg">
                <span class="close"></span>
            </div>
        </script>
  </body>
</html>
